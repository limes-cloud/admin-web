		"arco-design-pro-vue": "^2.7.2",
		"axios": "^0.24.0",
		"cron-parser": "^4.9.0",
		"crypto-js": "^4.2.0",
		"dayjs": "^1.11.5",
		"echarts": "^5.4.0",
		"guid-typescript": "^1.0.9",
		"js-yaml": "^4.1.0",
		"jsencrypt": "^3.3.2",
		"lodash": "^4.17.21",
		"mitt": "^3.0.0",
		"monaco-editor": "^0.44.0",
		"nprogress": "^0.2.0",
		"pinia": "^2.0.23",
		"postcss-less": "^6.0.0",
		"query-string": "^8.0.3",
		"sortablejs": "^1.15.0",
		"stylelint-config-recommended-less": "^2.0.0",
		"tinymce": "6",
		"vue": "^3.4.27",
		"vue-echarts": "^6.2.3",
		"vue-i18n": "^9.2.2",
		"vue-router": "^4.0.14",
		"web-vue@^2.54.0": "link:arco-design/web-vue@^2.54.0",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@arco-plugins/vite-vue": "^1.4.5",
